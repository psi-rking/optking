# from __future__ import print_function
#! SCF cc-pVDZ geometry optimzation, with Z-matrix input
import optking

# nucenergy = 9.3007        
REF_energy = -76.027053513 

molecule h2o {
     O
     H 1 1.0
     H 1 1.0 2 104.5
}

set {
  diis false
  basis cc-pvdz
  e_convergence 10
  d_convergence 10
  g_convergence gau_tight
  scf_type pk
}

result = optking.optimize_psi4('scf')

#compare_values(nucenergy, h2o.nuclear_repulsion_energy(), 4, "Nuclear repulsion energy")    #TEST
compare_values(REF_energy, result['energies'][-1], 9, "Reference energy")                                #TEST
